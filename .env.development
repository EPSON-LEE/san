# 开发环境配置
NODE_ENV=development

# 服务端口配置
BACKEND_PORT=4000
FRONTEND_PORT=3000

# 数据库配置 (开发环境使用本地数据库)
MONGODB_VERSION=6.0
MONGODB_PORT=27017
MONGODB_USERNAME=dev_user
MONGODB_PASSWORD=dev_password
MONGODB_DATABASE=decode_dev
DATABASE_URL=mongodb://localhost:27017/decode_dev

# API 配置
API_PREFIX=api
REACT_APP_API_URL=http://localhost:4000

# 日志配置 (开发环境使用详细日志)
LOG_LEVEL=debug

# JWT 配置 (开发环境使用简单密钥)
JWT_SECRET=dev-jwt-secret

# 其他配置
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW_MS=900000