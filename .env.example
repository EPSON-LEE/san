# 应用环境
NODE_ENV=production

# 服务端口配置
SERVER_PORT=4000
PC_PORT=80
H5_PORT=8080

# 数据库配置
MONGODB_URI=mongodb://root:example@mongodb:27017/decode?authSource=admin
MONGODB_USER=root
MONGODB_PASSWORD=example
MONGODB_PORT=27017
MONGODB_DATABASE=decode

# API配置
API_URL=http://api.example.com
API_PREFIX=/api

# 日志配置
LOG_LEVEL=info

# JWT配置
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=7d

# 其他配置
ENABLE_SWAGGER=false
CORS_ORIGIN=*